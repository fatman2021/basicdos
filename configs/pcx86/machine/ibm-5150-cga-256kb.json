{
  "name": "IBM PC, 256Kb RAM, Dual Floppy Drives, CGA, Debugger",
  "defaults": {
    "ram": "256K",
    "fpu": "None",
    "fdc": "2 diskette drives",
    "parallel": "LPT1",
    "serial": "COM1"
  },
  "machine": {
    "type": "pcx86",
    "name": "PCx86",
    "version": "2.04",
    "factory": "embedPCx86",
    "script": "/software/pcx86/releases/pcx86.js"
  },
  "components": [
    {
      "id": "pcCGA",
      "class": "computer"
    },
    {
      "id": "romBASIC",
      "class": "rom",
      "addr": "0xf6000",
      "size": "0x08000",
      "alias": null,
      "file": "/software/pcx86/roms/BASIC100.json"
    },
    {
      "id": "romBIOS",
      "class": "rom",
      "addr": "0xfe000",
      "size": "0x02000",
      "file": "/software/pcx86/roms/PCBIOS-REV1.json"
    },
    {
      "id": "videoCGA",
      "class": "video",
      "title": "Color Display",
      "screenWidth": 1280,
      "screenHeight": 800,
      "screenColor": "black",
      "charCols": 80,
      "charRows": 25,
      "fontROM": "/software/pcx86/roms/IBM-CGA.json",
      "flicker": 0,
      "overlay": true,
      "autoLock": true,
      "touchScreen": "mouse",
      "controls": [
        {
          "type": "group",
          "style": {
            "float": "right"
          }
        },
        {
          "id": "caps-lock",
          "type": "led",
          "title": "Caps",
          "style": {
            "padding-left": "8px"
          }
        },
        {
          "id": "num-lock",
          "type": "led",
          "title": "Num",
          "style": {
            "padding-left": "8px"
          }
        },
        {
          "id": "scroll-lock",
          "type": "led",
          "title": "Scroll",
          "style": {
            "padding-left": "8px"
          }
        },
        {
          "id": "fullScreen",
          "type": "button",
          "label": "Full Screen",
          "style": {
            "padding-left": "8px",
            "line-height": "1em"
          }
        }
      ]
    },
    "/configs/pcx86/machine/fdc.json",
    {
      "id": "ramLow",
      "class": "ram",
      "test": "false",
      "controls": [
        {
          "id": "listSizes",
          "type": "list",
          "options": {
            "0": "Default",
            "16384": "16K",
            "32768": "32K",
            "49152": "48K",
            "65536": "64K",
            "98304": "96K",
            "131072": "128K",
            "163840": "160K",
            "196608": "192K",
            "229376": "224K",
            "262144": "256K",
            "294912": "288K",
            "327680": "320K",
            "360448": "352K",
            "393216": "384K",
            "425984": "416K",
            "458752": "448K",
            "491520": "480K",
            "524288": "512K",
            "557056": "544K",
            "589824": "576K",
            "622592": "608K",
            "655360": "640K"
          },
          "style": {
            "clear": "both"
          }
        }
      ]
    },
    {
      "id": "cpu8088",
      "class": "cpu",
      "model": 8088,
      "autoStart": true,
      "controls": [
        {
          "id": "reset",
          "type": "button",
          "label": "Reset",
          "style": {
            "padding-right": "8px"
          }
        }
      ]
    },
    "/configs/pcx86/machine/keys.json",
    {
      "id": "chipset",
      "class": "chipset",
      "model": 5150,
      "scaleTimers": false,
      "sw1": "01001001",
      "sw2": "10011000",
      "@note": "The above switch settings indicate '256K, No FPU, Color Monitor, 2 Floppy Drives'; see 'swdesc' below.",
      "sound": true,
      "floppies": "",
      "monitor": "",
      "dateRTC": ""
    },
    {
      "id": "lpt1",
      "class": "parallel",
      "adapter": 2,
      "binding": "print"
    },
    {
      "id": "com1",
      "class": "serial",
      "adapter": 1,
      "binding": "print"
    }
  ]
}
